/* pages/my/myShow/myShow.wxss */
page{
  height:100%;
}
.container .bot{
  display: block;
  line-height:120rpx;
  text-align: center;
}
.container text{
  display:block;
  width:100%;
  line-height:108rpx;
}
.container .box{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width:100%;
  height:100%;
}
.container .box .imgList{
  width:324rpx;
  height:324rpx;
  margin-bottom:36rpx;
}
.container .box .imgList:not(:first-child){
  background-color:#f1f1f4;
}
.container .box .icon-shangchuantupian{
  width:300rpx;
  height:300rpx;
  font-size:300rpx;
  padding:0;
  margin:0;
  line-height:320rpx;
}
.container .box image{
  width:100%;
  height:100%;
}